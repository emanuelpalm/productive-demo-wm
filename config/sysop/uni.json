{
  "serviceRegistryHost": "service-registry.uni:8443",
  "providers": [
    {
      "address": "sys-buyer.uni",
      "authenticationInfo": "",
      "port": 9001,
      "systemName": "buyer"
    },
    {
      "address": "sys-seller.uni",
      "authenticationInfo": "",
      "port": 9002,
      "systemName": "seller"
    }
  ],
  "services": [
    {
      "serviceDefinition": "event-subscriber",
      "providerIndex": 0,
      "serviceUri": "/events/buyer",
      "secure": "CERTIFICATE",
      "interfaces": [
        "HTTP-SECURE-JSON"
      ]
    },
    {
      "serviceDefinition": "buyer-back-end",
      "providerIndex": 0,
      "serviceUri": "/",
      "secure": "TOKEN",
      "interfaces": [
        "HTTP-SECURE-JSON"
      ]
    },
    {
      "serviceDefinition": "event-subscriber",
      "providerIndex": 1,
      "serviceUri": "/events/seller",
      "secure": "CERTIFICATE",
      "interfaces": [
        "HTTP-SECURE-JSON"
      ]
    }
  ],
  "rules": [
    {
      "consumer": "buyer",
      "services": [
        "event-subscribe",
        "event-unsubscribe"
      ],
      "providers": [
        "event_handler"
      ]
    },
    {
      "consumer": "buyer",
      "services": [
        "trusted-contract-negotiation",
        "trusted-contract-observation"
      ],
      "providers": [
        "contract_proxy_buyer"
      ]
    },
    {
      "consumer": "contract_proxy_buyer",
      "services": [
        "contract-negotiation"
      ],
      "providers": [
        "contract_proxy_seller"
      ]
    },
    {
      "consumer": "contract_proxy_buyer",
      "services": [
        "event-publish"
      ],
      "providers": [
        "event_handler"
      ]
    },
    {
      "consumer": "contract_proxy_seller",
      "services": [
        "contract-negotiation"
      ],
      "providers": [
        "contract_proxy_buyer"
      ]
    },
    {
      "consumer": "contract_proxy_seller",
      "services": [
        "event-publish"
      ],
      "providers": [
        "event_handler"
      ]
    },
    {
      "consumer": "seller",
      "services": [
        "event-subscribe",
        "event-unsubscribe"
      ],
      "providers": [
        "event_handler"
      ]
    },
    {
      "consumer": "seller",
      "services": [
        "trusted-contract-negotiation",
        "trusted-contract-observation"
      ],
      "providers": [
        "contract_proxy_seller"
      ]
    }
  ]
}